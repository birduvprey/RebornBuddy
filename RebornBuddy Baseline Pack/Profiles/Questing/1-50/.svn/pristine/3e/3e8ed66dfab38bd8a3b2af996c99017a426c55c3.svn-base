   <Order>
      <BotSettings AutoEquip="1" />
      <!-- Coming to Gridania -->
      <If Condition="not IsQuestCompleted(65575)">
         <If Condition="not HasQuest(65575)">
            <WaitWhile Condition="Managers.QuestLogManager.InCutscene" />
            <!-- Select Mouse Controls -->
            <SelectStringClick Slot="0" />
            <MoveTo Name="Bertennant" Distance="1" UseMesh="False" XYZ="118.1308, -12.43027, 142.6035" />
            <If Condition="IsQuestAcceptQualified(65575)">
               <PickupQuest NpcId="1001148" QuestId="65575" XYZ="117.1129, -12.50626, 143.6331" />
            </If>
         </If>
         <If Condition="GetQuestStep(65575) == 255">
            <!-- <MoveTo Name="Mother Miounne" Distance="1" UseMesh="False" XYZ="61.87421, -6.129399, 92.88789" />
				<MoveTo Name="Mother Miounne" Distance="1" UseMesh="False" XYZ="25.30511, -8, 113.7628" /> -->
            <TurnIn QuestId="65575" NpcId="1001140" XYZ="23.78882, -8, 115.8617" />
         </If>
      </If>
      <!-- Close to Home - Archer -->
      <If Condition="Core.Player.CurrentJob == Enums.ClassJobType.Archer">
         <If Condition="not IsQuestCompleted(65659)">
            <If Condition="not HasQuest(65659)">
               <MoveTo Name="Mother Miounne" Distance="1" UseMesh="False" XYZ="25.30511, -8, 113.7628" />
               <If Condition="IsQuestAcceptQualified(65659)">
                  <PickupQuest NpcId="1001140" QuestId="65659" XYZ="23.81927, -8, 115.9227" />
               </If>
            </If>
            <If Condition="GetQuestStep(65659) == 1">
               <WaitWhile Condition="IsOnMap(183)" />
               <If Condition="not Managers.WorldManager.HasAetheryteId(2)">
                  <UseObject NpcId="2" UseTimes="1" BlacklistAfter="True" UseDistance="10" XYZ="32.9137, 2.670288, 30.0144" Radius="10" WaitTime="5000" Condition="not Managers.WorldManager.HasAetheryteId(2)" />
                  <UseTransport NpcId="2" UseTimes="1" DialogOption="2" BlacklistAfter="True" UseDistance="10" XYZ="32.9137, 2.670288, 30.0144" Radius="10" WaitTime="5000" />
               </If>
               <TalkTo InteractDistance="3.0" NpcId="1000197" XYZ="198.565, 0, 43.97984" />
            </If>
         </If>
         <!-- Way of the Archer -->
         <If Condition="not IsQuestCompleted(65557)">
            <If Condition="not HasQuest(65557)">
               <MoveTo Name="Athelyna" XYZ="198.565, 0, 43.97984" />
               <If Condition="IsQuestAcceptQualified(65557)">
                  <PickupQuest NpcId="1000197" QuestId="65557" XYZ="201.3123, 7.450581E-09, 43.90015" />
               </If>
            </If>
            <If Condition="GetQuestStep(65557) == 1">
               <UseTransport InteractDistance="3.0" NpcId="1000200" XYZ="209.5521, 0.9999819, 35.01941" />
            </If>
         </If>
         <If Condition="not IsQuestCompleted(65659)">
            <If Condition="GetQuestStep(65659) == 1">
               <If Condition="IsOnMap(132)">
                  <MoveTo Name="Old Gridania" Distance="0" XYZ="102.8434, 4.934001, 11.63276" />
               </If>
               <If Condition="IsOnMap(133)">
                  <HandOver ItemId="2000119" QuestId="65659" StepId="1" NpcId="1000768" XYZ="172.3506, 15.5, -89.95197" />
               </If>
            </If>
            <If Condition="GetQuestStep(65659) == 255">
               <If Condition="IsOnMap(133)">
                  <MoveTo Name="New Gridania" Distance="0" XYZ="137.2878, 11.10256, -18.40056" />
               </If>
               <If Condition="IsOnMap(132)">
                  <TurnIn QuestId="65659" NpcId="1000100" XYZ="23.81927, -8, 115.9227" />
               </If>
            </If>
         </If>
         <If Condition="not IsQuestCompleted(65557)">
            <If Condition="GetQuestStep(65557) == 2">
               <If Condition="IsOnMap(132)">
                  <MoveTo Name="Central Shroud" Distance="0" XYZ="158.5835, -12.85194, 157.9362" />
               </If>
               <If Condition="IsOnMap(148)">
                  <If Condition="HasQuest(65557)">
                     <Grind grindRef="WayOfTheArcher_GroundSquirrel" while="HasQuest(65557) and GetQuestById(65557).QuestI8AL &lt; 3" />
                     <Grind grindRef="WayOfTheArcher_LittleLadybug" while="HasQuest(65557) and GetQuestById(65557).QuestI8BH &lt; 3" />
                     <Grind grindRef="WayOfTheArcher_ForestFunguar" while="HasQuest(65557) and GetQuestStep(65557) == 2" />
                  </If>
               </If>
            </If>
            <If Condition="GetQuestStep(65557) == 255">
               <If Condition="IsOnMap(148)">
                  <MoveTo Name="New Gridania" Distance="0" XYZ="129.6041, 25.28367, -320.0131" />
               </If>
               <If Condition="IsOnMap(132)">
                  <TurnIn QuestId="65557" NpcId="1000200" XYZ="209.5521, 0.9999819, 35.01941" />
               </If>
            </If>
         </If>
      </If>
      <!-- Close to Home - Conjurer -->
      <If Condition="Core.Player.CurrentJob == Enums.ClassJobType.Conjurer">
         <If Condition="not IsQuestCompleted(65660)">
            <If Condition="not HasQuest(65660)">
               <MoveTo Name="Mother Miounne" Distance="1" UseMesh="False" XYZ="25.30511, -8, 113.7628" />
               <If Condition="IsQuestAcceptQualified(65660)">
                  <PickupQuest NpcId="1001140" QuestId="65660" XYZ="23.81927, -8, 115.9227" />
               </If>
            </If>
            <If Condition="GetQuestStep(65660) == 1">
               <WaitWhile Condition="IsOnMap(183)" />
               <If Condition="not Managers.WorldManager.HasAetheryteId(2)">
                  <UseObject NpcId="2" UseTimes="1" BlacklistAfter="True" UseDistance="10" XYZ="32.9137, 2.670288, 30.0144" Radius="10" WaitTime="5000" Condition="not Managers.WorldManager.HasAetheryteId(2)" />
                  <UseTransport NpcId="2" UseTimes="1" DialogOption="2" BlacklistAfter="True" UseDistance="10" XYZ="32.9137, 2.670288, 30.0144" Radius="10" WaitTime="5000" />
               </If>
               <If Condition="IsOnMap(132)">
                  <MoveTo Name="Old Gridania" Distance="0" XYZ="-106.191, 1.08756, 5.450311" />
               </If>
               <If Condition="IsOnMap(133)">
                  <TalkTo InteractDistance="5.0" NpcId="1000323" XYZ="-234.0276, -4, -11.09338" />
               </If>
            </If>
         </If>
         <!-- Way of the Conjurer -->
         <If Condition="not IsQuestCompleted(65558)">
            <If Condition="not HasQuest(65558)">
               <If Condition="IsOnMap(132)">
                  <MoveTo Name="Old Gridania" Distance="0" XYZ="-106.191, 1.08756, 5.450311" />
               </If>
               <MoveTo Name="Madelle" Distance="5" XYZ="-234.0276, -4, -11.09338" />
               <If Condition="IsQuestAcceptQualified(65558)">
                  <PickupQuest NpcId="1000323" QuestId="65558" XYZ="-234.0276, -4, -11.09338" />
               </If>
            </If>
            <If Condition="GetQuestStep(65558) == 1">
               <UseTransport InteractDistance="3.0" NpcId="1000692" XYZ="-258.8083, -5.773526, -27.26788" />
            </If>
         </If>
         <If Condition="not IsQuestCompleted(65660)">
